<!--admin.ejs-->
<style>
    tr:nth-child(odd) {background-color: #f2f2f2;}
</style>

<table class="table">
    <% events.forEach(function(model) { %>
    <tr>
        <td>
            <%= model.id %>
        </td>
        <td>
            <%= model.name %>
        </td>
    </tr>
    <% }); %>
</table>