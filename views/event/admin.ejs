<!--admin.ejs-->
<style>
    tr:nth-child(odd) {
        background-color: #f2f2f2;
    }
</style>

<table class="table">
    <% events.forEach(function(model) { %>
    <tr>
        <td>
            <%= model.id %>
        </td>
        <td>
            <a href="http://localhost:1337/event/admin/update/<%= model.id %>">
                <%= model.name %>
            </a>
        </td>
    </tr>
    <% }); %>
</table>